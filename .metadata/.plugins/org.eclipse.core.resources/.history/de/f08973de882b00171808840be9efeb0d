package newFeatures;

interface Formula {
	
	public double calculate(int a);
	
	default double sqrt(int a){
		return Math.sqrt(a);
	}
}

class Test implements Formula{
	
		@Override
		public double calculate(int a) {
			
			return sqrt(a*100);
		}
		
		public static void main(String[] args) {
			
			Formula formula = new Test();
			
			formula.calculate(100);
			formula.sqrt(16);
		}

	
}