package String;

public class StringDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * String类的特点：
		 * 
		 * 字符串对象一旦被初始化就不会被改变
		 * 
		 * 
		 * */
		StringDemo1();
	}

	public static void StringDemo1() {
		String s = "abc";//存储在字符串常量池中
//		s = "nba";
		
		String s1 = "abc";
		System.out.println(s==s1);//true
//		System.out.println("s=" + s);
	}

}
