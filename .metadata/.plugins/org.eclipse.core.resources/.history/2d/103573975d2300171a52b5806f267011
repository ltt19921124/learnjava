package String;

public class StringMethodDemo {
	
	public static void main(String[] args){
		
		/*
		 * 字符串功能进行分类
		 * "abcd"
		 * 1,获取
		 * 
		 * 
		 * 
		 * */
	
		stringMethodDemo_1();
		
	}

	private static void stringMethodDemo_1() {
		
		String s = "abcde";
		System.out.println("length:" + s.length());//
		
		
	}
	
}
