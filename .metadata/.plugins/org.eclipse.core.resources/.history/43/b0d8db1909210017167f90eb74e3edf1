package duoxiancheng;


class Demo_2 implements Runnable {
	
	public void run(){
		for(int x = 0;x < 10;x++){
			System.out.println(Thread.currentThread().getName() + x);
		}
	}
}

public class ThreadDemo3 {
	
	public static void main(String[] args){
		Demo_2 d1 = new Demo_2();
		Demo_2 d1 = new Demo_2();
		
		
	}
	
}
