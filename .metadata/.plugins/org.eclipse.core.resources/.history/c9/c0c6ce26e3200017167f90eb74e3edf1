package duoxiancheng;


class Demo_1 {
	private String name;
//	Demo_1() {
//		
//	}
	Demo_1(String name) {
		this.name = name;
	}
	public void show() {
		for (int i = 0; i < 10; i++) {
			System.out.println(name + "...x=" + i);
		}
	}
	
}

public class ThreadDemo2 {
	/*
	 *
	 * 
	 **/
	public static void main(String[] args){
		Demo_1 d1 = new Demo_1("Íú²Æ");
		Demo_1 d2 = new Demo_1("xiaoqiang");
		d1.show();
		d2.show();
		
	}
}
