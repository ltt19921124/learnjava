package yichang;


class Demo {
	public void method(int[] arr,int index) {
		
		System.out.println(arr[index]);
		System.out.println("haha");
	}
}

public class ExceptionDemo2 {
	public static void main(String[] args){
		int[] arr = new int[3];
		
		Demo d = new Demo();
		d.method(arr, 3);
		System.out.println("over");
		
	}
	
	
	
}
