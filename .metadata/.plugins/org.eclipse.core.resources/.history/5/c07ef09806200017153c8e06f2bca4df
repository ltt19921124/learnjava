


abstract class Demo{
	abstract void show();
}

class DemoA extends Demo{
	void show(){
		System.out.println("demoa show");
	}
}

class DemoB extends Demo{
	void show(){
		System.out.println("demob show");
	}
}

public class AbstractDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
