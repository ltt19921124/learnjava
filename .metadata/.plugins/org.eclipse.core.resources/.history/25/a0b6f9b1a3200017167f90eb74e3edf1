package yichang;


public class ExceptionDemo {
	public static void main(String[] args){
		
		int[] arr = new int[3];
		arr = null;
		System.out.println(arr[2]);//角标越界
		
	}
	
	public static void sleep(int time){
		if(time < 0) {
			//处理办法
			
		}
		if(time > 0) {
			//处理办法
		}
		System.out.println("shui..." + time);
	}
	
}
