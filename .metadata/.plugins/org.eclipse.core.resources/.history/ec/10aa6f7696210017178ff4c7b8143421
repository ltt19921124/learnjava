package duoxiancheng;

//
class Single {
	private static final Single s = new Single();
	private Single(){
		
	}
	public static Single getInstance() {
		return s;
	}
}
//

class Single_1{
	private static Single_1 s = null;
	
	private Single_1(){
		
	}
	public static Single getInstance() {
		if(s == null) {
			s = new Single_1();
		} else {
			return s;
		}
	}
	
}

public class SingleDemo {
	
	public static void main(String[] args) {
		
	}
	
}
