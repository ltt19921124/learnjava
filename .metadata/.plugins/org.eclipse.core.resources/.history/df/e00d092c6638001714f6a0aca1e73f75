package Chapter5_Extends;

class Base {
	
	public double size;
	public String name;
	
	public Base(double size,String name) {
		
		this.size = size;
		this.name = name;
		
	}

}

class Sub extends Base {
	
	public String color;
	public Sub(double size,String name,String color) {
		
		super(size,name);
		this.color = color;
		
	}
	public static void main(String[] args) {
		
		
	}
}














