package Chapter6;

public class Primitive2Wrapper {

	public static void main(String[] args) {
		
		boolean b1 = true;
		Boolean blObj = new Boolean(b1);
		int it = 5;
		Integer itObj = new Integer(it);
		Float fl = new Float("4.56");
		Boolean bObj = new Boolean("false");
		//
		Long lObj = new Long("ddd");
		
	}

}
