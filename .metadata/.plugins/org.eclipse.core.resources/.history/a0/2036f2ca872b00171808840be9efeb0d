package newFeatures;

interface Formula {
	
	public double calculate(int a);
	
	default double sqrt(int a){
		return Math.sqrt(a);
	}
}

class Test implements Formula{
	public static void main(String[] args) {
		Formula formula = new Formula() {
			
			public double calculate(int a) {
				return sqrt(a*100);
			}
		};
	}

	@Override
	public double calculate(int a) {
		
		return 0;
	}
}