package IO;

public class PersonDemo {

	public static void main(String[] args) {
		
		Person p = new Person();
		p.chifan();
	}

}

class Person {
	
	void chifan() {
		System.out.println("吃饭");
	}
}

//增强person而出现的

class NewPerson {
	private Person p;
	
	NewPerson(Person p) {
		this.p = p;
	}
	
	public void chifan() {
		System.out.println("开胃酒");
	}
	
}