package Chapter5_Extends;

public class Person {
	
	//定义第一个初始化块
	{
		int a = 6;
		if(a > 4) {
			System.out.println("Person初始化块:局部变量a的值大于4");
		}
	}
	
	//定义第二个初始化块
	{
		System.out.println("Person的第二个初始化块");
	}
	
	//定义无参数的构造器
	public Person() {
		
	}
	//
	public static void main(String[] args) {

	}
	

}
