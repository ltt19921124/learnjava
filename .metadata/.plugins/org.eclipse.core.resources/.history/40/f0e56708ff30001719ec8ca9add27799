import java.util.Date;

public class DateDemo {

	public static void main(String[] args) {
		
		long time = System.currentTimeMillis();
		System.out.println(time);
//		System.out.println();
		
		
		Date date = new Date();
		
		System.out.println(date);//Fri May 05 03:20:51 CST 2017
		
		Date date2 = new Date();
	}

}
