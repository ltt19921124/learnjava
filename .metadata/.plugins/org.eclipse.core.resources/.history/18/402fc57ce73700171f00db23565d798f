package Chapter5;

public class TestPerson1 {

	public static void main(String[] args) {
		
		System.out.println("person1类的eyeNum类属性值:" + Person1.eyeNum);
		//
		Person1 p = new Person1();
		System.out.println("p变量的name属性值是:" + p.name + "p对象的eyeNum属性值是:" + p.eyeNum);
		p.name = "孙悟空";
		p.eyeNum = 2;
		System.out.println("p变量的name属性值是:" + p.name + "p对象的eyeNum属性值是:" + p.eyeNum);
		
	}
	

}
